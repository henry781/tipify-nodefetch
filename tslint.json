{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-empty-interface": false,
    "interface-name": false,
    "no-empty": false,
    "no-string-literal": false,
    "no-unused-expression": false,
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      140
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
